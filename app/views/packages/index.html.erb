<h1>Packages#index</h1>
<p>Find me in app/views/packages/index.html.erb</p>


<h1>Package Listing</h1>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Destination</th>
      <th>Description</th>
      <th>Price</th>
      <th>Attraction</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @packages.each do |package| %>
    <tr>
      <td><%= package.ticket.airline %></td>
      <td><%= link_to package.ticket.destination.name, package %></td>
      <td><%= package.description %></td>
      <td><%= package.ticket.price %></td>
      <% package.attractions.each do |attraction| %>
      <td><%= attraction.name %></td>
      <td><%= package.status %></td>
      <td></td>
    </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<!-- modal goes here -->

<li><a href="#contact" data-toggle="modal">Contact</a></li>
<div class="modal fade" id="contact" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <form class="form-horizontal">
      <div class="modal-header">
        <h4>Contact Tech Site</h4>
      </div>
      <div class= "modal-body">

      <div class="form-group">
        <label class= "contact Name" class = "col-lg-2 control-label">Name:</label>
        <div class = "col-lg-10">

        <input type = "text" class= "form-control" id= "contact-name" placeholder = "Full Name">
        </div>
      </div>

      </div>
      <div class="modal-footer">
        <a class="btn btn-default" data-dismiss = "modal">Close</a>
        <a class="btn btn-primary" data-dismiss = "modal">Sell</a>
      </div>
    </form>
  </div>

