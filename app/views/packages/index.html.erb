<h1>Packages#index</h1>
<p>Find me in app/views/packages/index.html.erb</p>

<h1>Package Listing</h1>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Destination</th>
      <th>Description</th>
      <th>Price</th>
      <th>Attraction</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @packages.each do |package| %>
    <tr>
      <td><%= package.ticket.airline %></td>
      <td><%= package.ticket.destination.name %></td>
      <td><%= package.description %></td>
      <td><%= package.ticket.price %></td>
      <% package.attractions.each do |attraction| %>
      <td><%= attraction.name %></td>
      <td></td>
      <td></td>
    </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
