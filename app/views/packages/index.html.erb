<h1>Package Listing</h1>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Destination</th>
      <th>Description</th>
      <th>Price</th>
      <th>Purchase</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @packages.each do |package| %>
    <tr>
      <td><%= package.ticket.airline %></td>
      <td><%= package.ticket.destination %></td>
      <td><%= package.description %></td>
      <td><%= package.ticket.price %></td>
      <td></td>
      <td></td>

    </tr>
    <% end %>
  </tbody>
</table>
